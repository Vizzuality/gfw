{
  "baseUrl": "http://localhost:3000",
  "defaultCommandTimeout": 100000,
  "responseTimeout": 100000,
  "env": {
    "codeCoverage": {
      "url": "/api/__coverage__"
    }
  },
  "ignoreTestFiles": [
    "**/__snapshots__/*",
    "**/__image_snapshots__/*"
  ],
  "lighthouse": {
    "performance": 0,
    "accessibility": 50,
    "best-practices": 85,
    "seo": 85,
    "pwa": 0
  }
}
